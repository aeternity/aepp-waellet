{
  "name": "waellet",
  "description": "waellet - aeternity browser extension",
  "version": null,
  "manifest_version": 2,
  "applications": {
    "gecko": {
      "id": "hello@waellet.com",
      "strict_min_version": "53.0"
    }
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "clipboardWrite",
    "activeTab",
    "webRequest",
    "notifications",
    "videoCapture"
  ],
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "waellet",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "content_scripts": [{
    "run_at": "document_start",
    "all_frames":true,
    "matches": [
      "https://*/*",
      "http://*/*"
    ],
    "js": ["inject.js"]
  }],
  "web_accessible_resources": [
    "inject.js",
    "phishing/phishing.html",
    "phishing/phishing.js",
    "aepp.js"
  ],
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  }
}
